  Title                    = {{Directional Distributional Similarity for Lexical Inference}},
  Author                   = {Kotlerman, Lili and Dagan, Ido and Szpektor, Idan and Zhitomirsky-Geffet, Maayan},
  Journal                  = {Natural Language Engineering},
  Year                     = {2010},
  Number                   = {4},
